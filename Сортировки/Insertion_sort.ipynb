    {
      "cell_type": "code",
      "source": [
        "for i in range(5):\n",
        "    j = i\n",
        "    while j > 0 and arr[j] < arr[j - 1]:\n",
        "        arr[j], arr[j - 1] = arr[j - 1], arr[j]\n",
        "        j = j - 1\n",
        "        print(arr)"
      ],
